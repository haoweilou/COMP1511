#include<stdio.h>
#define MAX 1000
int string_length(char ch[]){
    int i = 0, string_length=0;
    while(ch[i] != '\0'){
        i = i + 1;
        string_length = i;
    }
    return string_length;
}
int main(){
    char ch[] = "David is David";
    int a = string_length(ch);
    
    printf("string length is %d\n",a);
    printf("ch[%d] is %c\n",14,ch[14]);
    
    return 0;
}
